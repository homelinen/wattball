<h1>Results for Wattball Matches</h1>
<div class="well">
  <table class="table table-hover table-striped table-bordered table-clickable table-center">
    <thead>
      <tr class="info">
        <th>Team 1</th>
        <th>Result</th>
        <th>Team 2</th>
        <th>Date</th>
      </tr>
    </thead>
    <tbody>
  <% @wattball_matches.each do |match| %>
     <tr onclick="document.location = '<%= wattball_matches_path %>/<%= match.id %>';">
        <td><%= match.team1.teamName %></td>
        <td><%=  if match.result
                   match.result.join(" - ")
                 else
                   " - "
                 end
      %></td>
        <td><%= match.team2.teamName %></td>
        <td><%= get_date_for_event(match.event.start) %></td>
     </tr>
  <% end %>
    </body>
  </table>
</div>
<%= link_to 'Back', events_path %>
