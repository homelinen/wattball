<% t.column :event, :title => "Start" do |event| %>
    <% get_time_of_event event.start %>
  <% end %>
  <% t.column :event, :title => "End" do |event| 
    event.end
  end %>
  <% t.column :event, :title => "Date" do |event| 
    event.date.strftime "%d/%m/%y"
  end %>
  <% t.column :event, :title => "Status" do |event| 
    event.status
  end %>
  <% t.column :event, :title => "Referee" do |event| %>
    <% link_to join_user_name(event.official.user), official_path(event.official) %>
  <% end %>
  <% t.column :event, :title => "Tournament" do |event| %>
    <% event.tournament.name %>
  <% end %>
